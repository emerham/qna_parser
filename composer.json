{
  "name": "emerham/qnabot_parser",
  "description": "Parse the QnA Dump from Amazon's QnA Bot into a Spreadsheet for use in Botium Box.",
  "minimum-stability": "stable",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Matthew Brabham",
      "email": "brabham.matthew@gmail.com"
    }
  ],
  "require": {
    "ext-json": "*",
    "php": "^5.6.0|^7.0.0",
    "phpoffice/phpspreadsheet": "^1.6"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.4",
    "friendsofphp/php-cs-fixer": "^2.14"
  },
  "bin": [
    "bin/qnabotiumparse"
  ],
  "autoload": {
    "psr-4": {
      "Emerham\\": "src"
    }
  }
}
